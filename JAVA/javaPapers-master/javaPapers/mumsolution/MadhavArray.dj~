class MadhavArray
{
  static int isMadhavArray(int[]a)
  {
    int len=a.length;
    int x=0;
    boolean check=false;
    
    for(int n=1;n<=len/2;n++)
    {
      int s=n*(n+1)/2;
      if(s==len){
       check=true;
        break;
      }
    }
    
    if(check){
      for(int i=0;i<len;i++){
        
      }
    }
  }
  public static void main(String args[])
  {
    int[]m={6, 2, 4, 2, 2, 2, 1, 5, 0, 0};
    int x=isMadhavArray(m);
    System.out.println(x);
    
  }
}